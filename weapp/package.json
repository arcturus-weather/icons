  "dependencies": {
    "@mpflow/plugin-babel": "^0.0.37",
    "@mpflow/plugin-css": "^0.0.33",
    "@mpflow/plugin-typescript": "^0.0.33",
    "@mpflow/service": "^0.0.33",
    "miniprogram-api-typings": "^3.9.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.11.2",
    "@types/wechat-miniprogram": "^2.11.0",
    "typescript": "^3.9.7"
